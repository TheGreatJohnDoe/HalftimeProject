<table class="table">
    <thead>
        <tr>
            <th>Made by</th>
            <th>ID</th>
            <th>Created</th>
            <th>Type</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @using RequestsMaster.Models;
        @foreach (var x in (List<Request>)ViewData["requests"])
        {
            <tr>
                <td>
                    @x.userid
                </td>
                <td>
                    @x.id
                </td>
                <td>
                    @x.created
                </td>
                <td>
                    @x.req_type
                </td>
                <td>
                    @x.req_status
                </td>
            </tr>
        }
    </tbody>
</table>